algoritmo "05decisão"

var
  sexo:caractere

inicio
   escreval("Digite a sigla do seu sexo: ")
   leia (sexo)

   escolha sexo

   caso "M"
   escreval ("Masculino")
      // parar break
   caso "F"
   escreval("Feminina")
      // parar break
      
   outrocaso
   escreval ("Digite a sigla M ou F") //defaul/padrão

   fimescolha



fimalgoritmo